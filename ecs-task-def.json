{
    "family": "httpd-task",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "name": "yamanote_wasuremono_node",
            "image": "561712284442.dkr.ecr.ap-northeast-1.amazonaws.com/yamanote_wasuremon_oo",
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp"
                }
            ],
            "essential": true
        }
    ],
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    },
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "executionRoleArn": "ecsTaskExecutionRole",
    "cpu": "256",
    "memory": "512"
}
